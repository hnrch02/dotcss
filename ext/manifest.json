{
  "name"        : "dotcss",
  "version"     : "1.0",
  "description" : "~/.css",
  "icons": { "48" : "icon48.png", "128" : "icon128.png" },
  "content_scripts": [{
    "all_frames": true,
    "run_at"    : "document_idle",
    "matches"   : ["http://*/*", "https://*/*"],
    "js"        : ["jquery.js", "dotcss.js"]
  }],
  "permissions": ["tabs"]
}
